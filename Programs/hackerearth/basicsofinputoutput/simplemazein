#include<stdio.h>
#include<string.h>
int main()
{
	int a =0,b=0,i;
	char c[201];
	scanf("%s",&c);
	for(i=0;i<strlen(c);i++)
	{
		if(c[i]=='L')
		{
			a = a - 1;
		}
		else if(c[i]=='R')
		{
			a = a + 1;
		}
		else if(c[i]=='D')
		{
			b = b - 1;
		}
		else if(c[i]=='U')
		{
			b = b + 1;
		}
	}
	printf("%d %d",a,b);
}
