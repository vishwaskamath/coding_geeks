#include<stdio.h>
#include<string.h>
int main()
{
	char s[11];
	int i;
	scanf("%s",&s);

	int a = s[0] + s[1];
	int b = s[3] + s[4];
	int c = s[4] + s[5];
	int d = s[7] + s[8];

	if(s[2] == 'A' || s[2] == 'E' || s[2] == 'I' || s[2] == 'O' || s[2] == 'U' || s[2] == 'Y')
	{
		printf("invalid");
		
	}

	
	else if( ((a%2)==0) && ((b%2)==0) && ((c%2)==0) && ((d%2)==0) )
	{
		printf("valid");
	}
	else printf("invalid");
	
}
